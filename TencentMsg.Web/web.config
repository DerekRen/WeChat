<configuration>
	<system.web>
		<compilation debug="true" targetFramework="4.0"/>
      <pages validateRequest="false" enableEventValidation="false"/>
      <httpRuntime maxRequestLength="2097152" executionTimeout="360000" requestValidationMode="2.0"/>
	</system.web>
  <appSettings>
    <add key="TencentMsgData" value="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=|DataDirectory|\AccessData\TencentMsgData.accdb"  />
    <add key="File" value="/UploadFile"/> <!--上传路径-->
    <add key="LogFile" value="~"/>
    <!--上传路径-->
    <add key="appid" value="" /><!--配置appid的值-->
    <add key="appsecret" value="" /> <!--配置appsecret的值-->
    <!--配置appsecret的值-->
  </appSettings>
</configuration>
